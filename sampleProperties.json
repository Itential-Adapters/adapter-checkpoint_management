{
  "id": "Adapter-checkpoint_Management",
  "properties": {
    "protocol": "https",
    "host": "localhost",
    "port": 443,
    "version": "v1.0",
    "base_path": "/api",
    "stub": true,
    "authentication": {
      "username": "username",
      "password": "password",
      "auth_method": "basic user_password",
      "auth_field": "header.headers.Authorization",
      "auth_field_format": "Basic {b64}{username}:{password}{/b64}"
    },
    "healthcheck": {
      "type": "startup"
    },
    "request": {},
    "throttle": {
      "throttle_enabled": false
    },
    "proxy": {
      "enabled": false
    },
    "ssl": {
      "enabled": false
    },
    "cache_location": "none"
  },
  "type": "CheckpointManagement",
  "groups": [],
  "brokers": [],
  "logLevel": "none",
  "timeout": 120000
}